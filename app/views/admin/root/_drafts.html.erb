<table class="table table-striped table-bordered table-condensed" summary="List of &ldquo;Draft&rdquo; content">
  <thead>
    <tr>
      <th scope="col"><%= sortable "_type", "Format" %></th>
      <th scope="col"><%= sortable "business_proposition", "Biz?" %></th>
      <th scope="col" class="span4"><%= sortable "title" %></th>
      <th scope="col"><%= sortable "updated_at", "Updated" %></th>
      <th scope="col" class="span2"><%= sortable "assignee", "Assigned to" %></th>
      <th scope="col" class="span2"><%= sortable "creator", "Created by" %></th>
      <th scope="col"><%= sortable "section" %></th>
      <th scope="col">Edit</th>
      <th scope="col">Preview</th>
    </tr>
  </thead>
  <tbody>
    <%= render :collection => @presenter.draft, :partial => 'publication', :locals => {:tab => :draft, :with => with} %>
  </tbody>
</table>
