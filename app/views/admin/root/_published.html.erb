<h2>&ldquo;Published&rdquo; publications</h2>

<table class="table table-striped table-bordered table-condensed" summary="List of &ldquo;Published&rdquo; content">
  <thead>
    <tr>
      <th scope="col">Format</th>
      <th scope="col">Biz?</th>
      <th scope="col" class="span4">Title</th>
      <th scope="col">Updated</th>
      <th scope="col" class="span2">Assigned to</th>
      <th scope="col" class="span2">Created by</th>
      <th scope="col">Section</th>
      <th scope="col" class="span2">Published by</th>
      <th scope="col">View</th>
      <th scope="col">Edit</th>
      <th scope="col">New</th>
      <th scope="col">Preview</th>
    </tr>
  </thead>
  <tbody>
    <%= render :collection => @presenter.published, :partial => 'published_edition', :as => 'publication', :locals => {:tab => :published}  %>
  </tbody>
</table>